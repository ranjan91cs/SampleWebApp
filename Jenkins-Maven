pipeline{
    agent any
    
    environment{
    PATH = "/opt/maven/bin:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'github', url: 'https://github.com/ranjan91cs/SampleWebApp.git'
            }
        }
        stage("Maven Build"){
            steps{
            sh "mvn clean package"
            }
        }
    }
}
